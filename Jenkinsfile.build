def runScript = {
    sh "chmod +x ./file2.sh"
    sh "chmod +x ./file1.sh"
    sh """
    sh ./file2.sh ${env.PROD1} ${env.PROD2}
    """
}
pipeline {
    agent any 
    stages {
        stage('Stage 1') {
            environment{
                PROD1 = '''"""abc
                def
                ghi
                jkl"""'''
                PROD2 = 'Hi'
            }
            steps {
                script {runScript()}
            }
        }
    }
}
